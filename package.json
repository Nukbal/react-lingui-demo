{
  "name": "react-lingui-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nukbal.github.io/react-lingui-demo",
  "dependencies": {
    "@lingui/core": "^3.0.0-3",
    "@lingui/react": "^3.0.0-3",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snap",
    "test": "react-scripts test",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@lingui/cli": "^3.0.0-3",
    "@lingui/macro": "^3.0.0-3",
    "@types/jest": "^23.3.10",
    "@types/lingui__core": "^2.2.0",
    "@types/lingui__macro": "^2.7.0",
    "@types/lingui__react": "^2.5.0",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "gh-pages": "^2.0.1",
    "jest-dom": "^3.0.0",
    "react-snap": "^1.23.0",
    "react-testing-library": "^5.4.0",
    "typescript": "^3.2.2"
  },
  "reactSnap": {
    "inlineCss": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**.{ts,tsx}",
      "!src/serviceWorker.ts",
      "!src/setupTests.ts",
      "!src/index.tsx"
    ]
  }
}
